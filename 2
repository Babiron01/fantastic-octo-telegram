const speed = prompt("Enter the speed of the car:");

if (speed < 70) {
  console.log("Ok");
} else {
  const overSpeedLimit = speed - 70;
  const demeritPoints = Math.floor(overSpeedLimit / 5);
  if (demeritPoints > 12) {
    console.log("License suspended");
  } else {
    console.log(`Points: ${demeritPoints}`);
  }
}
